Algoritmo productoDeUnNumero
	Definir num1 Como Entero;
	Definir doble Como Entero;
	Definir triple Como Entero;
	
	Escribir "Ingrese un numero ";
	Leer num1;
	
	doble = 2*num1;
	Escribir "El duplo de " (num1) " es " (doble);
	
	triple = 3*num1;
	Escribir "El Triple de " (num1) " es " (triple);
	
FinAlgoritmo
